<?xml version="1.0" ?>
<inputs>
    <!-- ====================================================   -->
    <!--                Domain Geometry                         -->
    <!-- ====================================================   -->
    <ny>                    201                 </ny>
    <nx>                    201                 </nx>
    <nz>                    201                 </nz>
    <radius>                3                   </radius>
    <composite>             0                   </composite>            <!-- Toggles composite blobs + cylinders -->
    <ncylinders>            20                  </ncylinders>   
    <cyl_proportion>        0.5               </cyl_proportion>       <!-- 0-1 value mass fraction of cylinders vs blobs-->
    <phi_max>               90                  </phi_max>
    <theta_max>             90                  </theta_max>
    <target_porosity>       0.9                 </target_porosity>
    <!-- ====================================================   -->
    <!--             Simulation Conditions                      -->
    <!-- ====================================================   -->
    <tau>                   2.0                 </tau>
    <deltaP>                1.0e-4               </deltaP>
    <C>                     1e4                 </C>                    <!-- Forcheimer Coefficient set to 0.0 to recover D'arcy-->
    <fNameIn>               porespy_palabos_geometry.dat</fNameIn>
    <fNameOut>              tmp                 </fNameOut>
    <max_iter>              1000                </max_iter>
</inputs>
